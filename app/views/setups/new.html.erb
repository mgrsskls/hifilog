<%= render layout: "shared/dashboard", locals: { active_dashboard_menu: @active_dashboard_menu } do %>
  <%= form_for @setup, html: { class: "EntityForm", method: :post } do |f| %>
    <% content_for :heading do %>
      <%= t('setup.new.heading') %>
    <% end %>

    <%= render "form", f: f, setup: @setup %>
  <% end %>
<% end %>
