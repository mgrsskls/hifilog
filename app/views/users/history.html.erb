<%= render layout: "users/shared/layout" do %>
  <% if @possessions.any? %>
    <%= render "shared/history",
      possessions: @possessions
    %>
  <% else %>
    <%= render "shared/empty_state", message: t('empty_state.public_profile', name: h(@user.user_name), pull_out: false) %>
  <% end %>
<% end %>
